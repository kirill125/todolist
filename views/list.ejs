    <%- include('header'); -%>
    <div class="box" id="heading"> 
        <h1><%= listTitle %></h1>
    </div>
    
    <div class="box">
        <% for (userItem of userItems) { %>
            <div class="item">
                <input type="checkbox">
                <p> <%= userItem %></p>
            </div>
        <% } %>   
        
        <form action="/" method="post" class="item">
            <input type="text" name="userItem" placeholder="New Item" autocomplete="off">
            <button type="submit" name="list" value="<%= listTitle %>">+</button>
        </form>   
    </div>
    <%- include('footer'); -%>
    
